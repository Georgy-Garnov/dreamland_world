{
// В этой таблице каждому уникальному ключу соответствует набор падежных окончаний.
// Ключ может быть чем угодно, меткой или числом. Этот ключ потом используют,
// чтобы сослаться на тип окончаний для среднего, муж., жен. рода и множ. числа:
// например, "смертельн(ое,ый,ая1,ые)". 

// neutral
    "ее":  "|ее|его|ему|ее|им|ем",
    "ье":  "|ье|его|ему|ье|ьим|ьем",    
    "ое":  "|ое|ого|ому|ое|ым|ом",    

// male    
    "ий1": "|ий|его|ему|ий|им|ем",
    "ий2": "|ий|ьего|ьему|ий|ьим|ьем",
    "ий3": "|ий|ого|ому|ий|им|ом",     
    "ый":  "|ый|ого|ому|ый|ым|ом",
    "ой":  "|ой|ого|ому|ой|ым|ом",    

// female    
    "ая1": "|ая|ую|ей|ую|ей|ей",    
    "ая2": "|ая|ую|ой|ую|ой|ой",  
    "ья":  "|ья|ью|ьей|ью|ьей|ьей",
    
// plural
    "ие":  "|ие|их|им|ие|ими|их",
    "ьи":  "|ьи|ьих|ьим|ьи|ьими|ьих",     
    "ые":  "|ые|ых|ым|ые|ыми|ых"
    
}
